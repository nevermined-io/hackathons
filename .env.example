# Nevermined Credentials (required)
# Get your API key from https://nevermined.app Settings > API Keys
NVM_API_KEY=nvm:your-api-key-here
NVM_ENVIRONMENT=sandbox
NVM_PLAN_ID=your-plan-id-here
NVM_AGENT_ID=your-agent-id-here

# LLM Provider
OPENAI_API_KEY=sk-your-openai-api-key

# Server Configuration
PORT=3000
SERVER_URL=http://localhost:3000

# AWS Credentials (for AgentCore deployment)
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
